{
  "name": "@shoutem/builder-sdk",
  "version": "0.1.0",
  "description": "SDK for Shoutem API",
  "files": [
    "build/lib"
  ],
  "author": "Shoutem",
  "scripts": {
    "clean": "rimraf ./build/*",
    "build": "npm run clean && npm run build-script && npm run build-lib",
    "build-script": "webpack --config ./bin/webpack.config.js",
    "build-lib": "babel src --out-dir build/lib --copy-files"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "eslint": "^0.21.2",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.8.4"
  },
  "dependencies": {
  }
}
